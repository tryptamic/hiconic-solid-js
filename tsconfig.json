{
    "compilerOptions": {
      "target": "esnext",
      "module": "esnext",
      "jsx": "preserve",
      "jsxImportSource": "solid-js",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "strict": true,
      "skipLibCheck": true,
      "types": ["vite/client", "vitest"],
      "paths": {
        "@public/*": ["./public/*"],
        //"@dev.hiconic/runtime": ["src/hiconic-runtime.ts"]
      },
      "baseUrl": "./src"
    },
    
    "include": ["src"]
}